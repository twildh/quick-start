{
	"manifest_version": 3,
	"name": "QuickStart",
	"version": "1.1.2",
	"description": "Bookmark-based browser start page",
	"author": "Timoth√©e Wildhaber and Samuel Meuli",
	"homepage_url": "https://github.com/twildh/quick-start",
	"permissions": ["bookmarks", "storage"],
	"icons": {
		"48": "icons/icon-48.png",
		"128": "icons/icon-128.png",
		"512": "icons/icon-512.png",
		"1024": "icons/icon-1024.png"
	},
	"background": {
		"service_worker": "background.js"
	},
	"chrome_url_overrides": {
		"newtab": "./index.html"
	},
	"options_ui": {
		"page": "./options.html",
		"browser_style": true
	}
}
